#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n,m;
    scanf("%d",&n);

    for(int i=1;i<11;i++)
    {
        m = n * i;
        printf("%d x %d = %d\n",n,i,m);
    }
    return 0;
}
